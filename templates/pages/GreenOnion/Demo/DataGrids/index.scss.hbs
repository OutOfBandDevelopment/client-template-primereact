.demo-ui-page {
  .p-panel {
    margin-bottom: 1rem;

    .p-panel-header {
      background: #f8f9fa;
      border-bottom: 1px solid #dee2e6;
    }
  }

  .p-tabview {
    .p-tabview-nav {
      background: #ffffff;
      border-bottom: 2px solid #dee2e6;
    }

    .p-tabview-panels {
      background: #ffffff;
      padding: 1rem;
    }
  }

  .p-datatable {
    .p-datatable-header {
      background: transparent;
      border: none;
      padding: 1rem 0;
    }
  }
}
